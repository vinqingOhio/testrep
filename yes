            task.wait(60)
            if #messageLog > 0 then
                local successCount = 0
                for _, url in ipairs(webhooks) do
                    if sendToWebhook(url) then
                        successCount += 1
                    end
                end
                if successCount > 0 then
                    table.clear(messageLog)
                end
            end
        end
    end)
    
    

    
