local SlappleTpFarm
    local ping = game.Players.LocalPlayer:GetNetworkPing() * 1000
    if ping >= 150 then
        Fluent:Notify({
            Title = "Your ping is high",
            Content = "Some features may not work well.",
            SubContent = "especially slap aura", 
