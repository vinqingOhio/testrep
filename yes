                            pcall(function() getgenv().VINQ_LOADED = false end)
                                theframeig:Destroy()
								task.wait(0.1)
								theframeig = nil
                            end 
                        }
                    }
                })
            end
        end
    else
        Window:Dialog({
            Title = "Failed to get HTTP requests",
            Content = [[
    If you are running an older version, we will not prompt you with any updates.
    Report this in the discord.]],
            Buttons = {
                { 
                    Title = "I understand",
